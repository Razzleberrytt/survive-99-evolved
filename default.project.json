{
  "$schema": "https://rojo.space/schema/project.json",
  "name": "survive-99-evolved",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Packages": {"$path": "Packages"},
      "Remotes": {"$path": "src/ReplicatedStorage/Remotes"},
      "Components": {"$path": "src/ReplicatedStorage/Components"},
      "Systems": {"$path": "src/ReplicatedStorage/Systems"},
      "Shared": {"$path": "src/ReplicatedStorage/Shared"},
      "Vendor": {"$path": "vendor"}
    },
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Server": {
        "$path": "src/ServerScriptService/Server",
        "Tests": {"$path": "test"}
      },
      "Services": {"$path": "src/ServerScriptService/Services"},
      "Actors": {"$path": "src/ServerScriptService/Actors"}
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Client": { "$path": "src/StarterPlayer/StarterPlayerScripts" }
      }
    }
  }
}
